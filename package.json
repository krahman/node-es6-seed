{
  "name": "node-es6-seed",
  "version": "0.0.1",
  "description": "Node ES6 Express Seed",
  "scripts": {
    "start": "node node_modules/babel-cli/bin/babel-node.js server/bin/www.js",
    "lint": "./node_modules/jshint/bin/jshint --reporter=node_modules/jshint-stylish  server/**/*.js",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register",
    "test-coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/mocha -- --compilers js:babel-core/register"
  },
  "keywords": [
    "Node",
    "es6",
    "express",
    "seed"
  ],
  "author": "Khalilur Rahman",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.4",
    "jshint": "^2.9.2",
    "jshint-stylish": "^2.2.0"
  }
}
